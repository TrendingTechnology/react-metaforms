{
  "name": "forms",
  "version": "0.1.0",
  "private": true,
  "main": "build/index.js",
  "jsnext:main": "src/Form.js",
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "enzyme": "^2.8.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-scripts": "0.9.5",
    "sinon": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "transpile": "babel src -d build --presets es2015,react,stage-0",
    "build": "rm -rf build && npm run transpile && rm build/index.js && mv build/Form.js build/index.js && find build -name '*.test.js' -type f -delete",
