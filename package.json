{
  "name": "@falsy/forms",
  "version": "0.1.2",
  "private": false,
  "main": "build/index.js",
  "jsnext:main": "src/Form.js",
  "files": [
    "build",
    "src"
  ],
  "peerDependencies": {
    "react": "^15.5.0"
  },
  "dependencies": {
    "prop-types": "15.5.8",
    "react": "15.5.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.8.1",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-scripts": "0.9.5",
    "react-test-renderer": "15.5.4",
    "sinon": "2.1.0"
  },
  "scripts": {
    "prepublish": "rm -rf build && npm run build",
    "start": "react-scripts start",
    "transpile": "babel src -d build --presets es2015,react,stage-0",
    "build": "rm -rf build && npm run transpile && rm build/index.js && mv build/export.js build/index.js && find build -name '*.test.js' -type f -delete",
    "test": "react-scripts test --env=jsdom",
    "coverage": "npm test -- --coverage"
  }
}
